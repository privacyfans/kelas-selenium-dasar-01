<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="All Test Suite" verbose="1" >

    <groups>
        <run>
            <include name="negativeTest"/>
        </run>
    </groups>

    <test name="Positive Test">
        <classes>
            <class name="com.herokuapp.internet.PositiveLoginIT"/>
        </classes>
    </test>

    <test name="Negative Test" >
        <classes>
            <class name="com.herokuapp.internet.NegativeLoginIT"/>
        </classes>
    </test>


</suite>
